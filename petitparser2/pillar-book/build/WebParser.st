'From Pharo5.0 of 16 April 2015 [Latest update: #50765] on 23 December 2016 at 5:09:41.484334 pm'!WebGrammar subclass: #WebParser	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PetitParser2-Tutorial'!!WebParser methodsFor: 'accessing' stamp: '11/16/2016 19:01:21'!structuredDocument	^ super structuredDocument		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself	]! !!WebParser methodsFor: 'accessing' stamp: 'JanKurs 11/21/2016 07:19'!elOpen	^ super elOpen trimRight! !!WebParser methodsFor: 'as yet unclassified' stamp: 'JanKurs 12/6/2016 23:44'!text	^ super text flatten		map: [ :_value | 		UnknownText new			text: _value;			yourself		]! !!WebParser methodsFor: 'as yet unclassified' stamp: '11/16/2016 19:01:21'!element	^ super element 		map: [ :_open :_content :_close | 	 	(HtmlElement new)			name: _open;			children: _content;			yourself	]! !!WebParser methodsFor: 'as yet unclassified' stamp: 'JanKurs 12/6/2016 23:43'!javascript	^ super javascript		map: [ :_code | 		(JavascriptElement new)			code: _code;			yourself	]! !!WebParser methodsFor: 'as yet unclassified' stamp: 'JanKurs 12/6/2016 23:43'!jsContent	^ super jsContent flatten! !!WebParser methodsFor: 'as yet unclassified' stamp: 'JanKurs 12/21/2016 19:32'!elClose	^ super elClose trim! !!WebParser methodsFor: 'as yet unclassified' stamp: '11/16/2016 19:01:21'!document	^ super document		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself		]! !