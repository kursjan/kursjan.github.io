'From Pharo5.0 of 16 April 2015 [Latest update: #50765] on 18 February 2017 at 9:36:53.007386 am'!WebGrammar subclass: #WebParser	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PetitParser2-Tutorial'!!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!jsContent	^ super jsContent flatten! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!elOpen	^ super elOpen trimRight! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!elClose	^ super elClose trim! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!element	^ super element 		map: [ :_open :_content :_close | 	 	(HtmlElement new)			name: _open;			children: _content;			yourself	]! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!structuredDocument	^ super structuredDocument		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself	]! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!text	^ super text flatten		map: [ :_value | 		UnknownText new			text: _value;			yourself		]! !!WebParser methodsFor: 'accessing' stamp: '1/26/2017 07:21:56'!javascript	^ super javascript		map: [ :_code | 		(JavascriptElement new)			code: _code;			yourself	]! !!WebParser methodsFor: 'various' stamp: '1/26/2017 07:21:56'!document	^ super document		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself		]! !