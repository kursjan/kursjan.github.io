'From Pharo5.0 of 16 April 2015 [Latest update: #50766] on 1 February 2017 at 7:56:07.239449 am'!WebGrammar subclass: #WebParser	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PetitParser2-Tutorial'!!WebParser methodsFor: 'various' stamp: ' 1/27/2017 07:41:38'!document	^ super document		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself		]! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!javascript	^ super javascript		map: [ :_code | 		(JavascriptElement new)			code: _code;			yourself	]! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!structuredDocument	^ super structuredDocument		map: [ :_bw :_document :_aw |		| beforeWater afterWater |		beforeWater := UnknownText new			text: _bw;			yourself.					afterWater := UnknownText new			text: _aw;			yourself.					HtmlElement new			name: 'DOCUMENT';			children: (Array with: beforeWater with: _document with: afterWater);			yourself	]! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!text	^ super text flatten		map: [ :_value | 		UnknownText new			text: _value;			yourself		]! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!element	^ super element 		map: [ :_open :_content :_close | 	 	(HtmlElement new)			name: _open;			children: _content;			yourself	]! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!jsContent	^ super jsContent flatten! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!elOpen	^ super elOpen trimRight! !!WebParser methodsFor: 'accessing' stamp: ' 1/27/2017 07:41:38'!elClose	^ super elClose trim! !