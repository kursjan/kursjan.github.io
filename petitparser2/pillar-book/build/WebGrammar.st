'From Pharo5.0 of 16 April 2015 [Latest update: #50766] on 24 February 2017 at 7:35:04.454142 am'!PP2CompositeNode subclass: #WebGrammar	instanceVariableNames: 'document javascript elOpen elContent elClose elementName element text jsOpen jsContent jsClose jsString structuredDocument comment any'	classVariableNames: ''	poolDictionaries: ''	category: 'PetitParser2-Tutorial'!!WebGrammar methodsFor: 'various' stamp: ' 1/27/2017 07:41:38'!structuredDocument	^ element sea! !!WebGrammar methodsFor: 'various' stamp: ' 1/27/2017 07:41:38'!any	^ #any asPParser! !!WebGrammar methodsFor: 'various' stamp: 'JanKurs 2/20/2017 07:23'!comment	^ '<!!--' asPParser, any starLazy, '-->' asPParser! !!WebGrammar methodsFor: 'various' stamp: ' 1/27/2017 07:41:38'!start	^ structuredDocument ! !!WebGrammar methodsFor: 'various' stamp: 'JanKurs 2/20/2017 07:23'!text	^ (comment / any) starLazy! !!WebGrammar methodsFor: 'various' stamp: ' 1/27/2017 07:41:38'!document	^ (javascript sea ==> #second) star! !!WebGrammar methodsFor: 'javascript' stamp: ' 1/27/2017 07:41:38'!jsString	^ $' asPParser, any starLazy, $' asPParser! !!WebGrammar methodsFor: 'javascript' stamp: 'JanKurs 2/14/2017 07:41'!jsContent	^ (jsString / any) starLazy! !!WebGrammar methodsFor: 'javascript' stamp: ' 1/27/2017 07:41:38'!jsOpen	^ '<script>' asPParser! !!WebGrammar methodsFor: 'javascript' stamp: ' 1/27/2017 07:41:38'!jsClose	^ '</script>' asPParser! !!WebGrammar methodsFor: 'javascript' stamp: ' 1/27/2017 07:41:38'!javascript	^ jsOpen, jsContent, jsClose ==> #second! !!WebGrammar methodsFor: 'element' stamp: ' 1/27/2017 07:41:38'!elContent	^ (javascript / element / text nonEpsilon) star! !!WebGrammar methodsFor: 'element' stamp: ' 1/27/2017 07:41:38'!element	^ (elOpen, elContent, elClose)"		memoize;"		yourself! !!WebGrammar methodsFor: 'element' stamp: 'JanKurs 2/20/2017 07:23'!elOpen	^ $< asPParser, elementName push, any starLazy, $> asPParser ==> #second! !!WebGrammar methodsFor: 'element' stamp: ' 1/27/2017 07:41:38'!elementName	^ #word asPParser plus flatten! !!WebGrammar methodsFor: 'element' stamp: ' 1/27/2017 07:41:38'!elClose	^ '</' asPParser, elementName match pop, $> asPParser! !